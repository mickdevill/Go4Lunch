<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/maps">

    <fragment
        android:id="@+id/maps"
        android:name="com.mickdevil.go4lunch.UI.botoomNavStaf.map.MapsFragment"
        android:label="@string/map_view"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.mickdevil.go4lunch.UI.botoomNavStaf.dashboard.DashboardFragment"
        android:label="@string/list_view"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.mickdevil.go4lunch.UI.botoomNavStaf.notifications.NotificationsFragment"
        android:label="@string/worckmates"
        tools:layout="@layout/fragment_notifications" />

</navigation>